{
  "name": "ts-boilerplate",
  "description": "typescript boilerplate",
  "version": "0.0.1",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "start": "tsc && yarn run build-ui && yarn run assets && electron .",
    "build": "tsc -p .",
    "build-ui": "cd svelte && yarn run build-svelte && cp -r -T ./dist/. ../ui && cd ..",
    "assets": "cp -r -T ./ui dist/ui",
    "fix": "electron-builder install-app-deps"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.5.0",
    "@types/chalk": "^2.2.0",
    "@types/express": "^4.17.13",
    "@types/fs-extra": "^8.1.0",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.11.0",
    "@types/socket.io": "^3.0.2",
    "electron": "^18.2.0",
    "electron-builder": "^23.0.3",
    "tslint": "^6.1.1",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "better-sqlite3": "^7.5.1",
    "chalk": "^4.0.0",
    "express": "^4.18.1",
    "fs-extra": "^9.0.0",
    "gkm": "^0.2.0",
    "lodash": "^4.17.15",
    "socket.io": "^4.5.0"
  }
}
